{
  "name": "Chrome extension example",
  "action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 3,
  "version": "0.1",
  "description": "Chrome extension example",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*/",
    "http://*/"
  ],
  "background": {
    "service_worker": "background.js"
  }
}